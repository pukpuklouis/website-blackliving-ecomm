---
import { generateCategoryStaticPaths } from '../../utils/productPaths.ts';

export const prerender = true;

export async function getStaticPaths() {
  const paths = await generateCategoryStaticPaths();
  return paths.map((path) => ({ params: path.params }));
}

const { category } = Astro.params;

return Astro.redirect(`/${category}`);
